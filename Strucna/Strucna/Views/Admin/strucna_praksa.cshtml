        <div id="content">

            <div class="inner">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3> Pregled prijava</h3>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-2">
                                        <div class="form-group">
                                            <label>Akademska godina:</label>
                                            <select class="form-control">
                                                <option>2017./2018.</option>
                                                <option>2016./2017.</option>
                                                <option>2015./2016.</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-5">
                                        <div class="form-group">
                                            <label>Naziv prakse:</label>
                                            <select class="form-control">
                                                <option>Praksa studenata računarstva </option>
                                                <option>Praksa studenata menadžmenta</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-5">
                                    </div>
                                </div> <!--END ROW -->
                                <br>
                                
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Naziv stručne prakse </th>
                                                <th>Naziv studija</th>
                                                <th>Ak.godina</th>
                                                <th>Mentor</th>
                                                <th>Trajanje</th>
                                                <th>Završena</th>
                                                <th>Aktivnost</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td class="col-md-1">1.</td>
                                                <td class="col-md-2">Praksa studenti menadžemnta</td>
                                                <td class="col-md-2">Računarstvo</td>
                                                <td class="col-md-1">2017./2018.</td>
                                                <td class="col-md-2">Bruno Trstenjak</td>
                                                <td class="col-md-1">Od:12.04.2017.<br>Do:01.03.2017</td>
                                                <td class="col-md-1">Ne</td>
                                                <td class="col-md-2">
                                                    <a href="#" class="btn btn-primary btn-xs btn-grad">Uredi</a>
                                                    <a href="#" class="btn btn-danger btn-xs btn-grad">Briši</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="col-md-1">2.</td>
                                                <td class="col-md-3">Praksa studenti menadžemnta</td>
                                                <td class="col-md-2">Menadžment</td>
                                                <td class="col-md-1">2017./2018.</td>
                                                <td class="col-md-2">Bruno Trstenjak</td>
                                                <td class="col-md-1">Od:12.04.2017.<br>Do:01.03.2017</td>
                                                <td class="col-md-1">Ne</td>
                                                <td class="col-md-1">
                                                    <a href="#" class="btn btn-primary btn-xs btn-grad">Uredi</a>
                                                    <a href="#" class="btn btn-danger btn-xs btn-grad">Briši</a>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>

                </div>

            </div>




        </div>
