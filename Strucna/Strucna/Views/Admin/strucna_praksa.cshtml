        <div id="left">
            <div class="media user-media well-small">
                <a class="user-link" href="#">
                    <img class="media-object img-thumbnail user-img" alt="User Picture" src="~/assets/img/user.png" />
                </a>
                <br />
                <div class="media-body">
                    <h5 class="media-heading"> PEDAGOG</h5>
                </div>
                <br />
            </div>

            <ul id="menu" class="collapse">


                <li class="panel">
                    <a href='@Url.Action("index_admin","Admin")'>
                        <i class="icon-table"></i> Početna stranica
                    </a>
                </li>
                <li class="panel ">
                    <a style="color:darkviolet;" href="#" data-parent="#menu" data-toggle="collapse" class="accordion-toggle" data-target="#component-nav">
                        <i class="icon-tasks"> </i> Mentori

                        <span class="pull-right">
                            <i class="icon-angle-left"></i>
                        </span>
                    </a>
                    <ul class="panel-collapse collapse in" id="component-nav">
                        @Html.Partial("partial_mentor")
                    </ul>
                </li>
                <li class="panel ">
                    <a href="#" data-parent="#menu" data-toggle="collapse" class="accordion-toggle" data-target="#component-rod">
                        <i class="icon-tasks"> </i> Studenti

                        <span class="pull-right">
                            <i class="icon-angle-left"></i>
                        </span>
                    </a>
                    <ul class="collapse" id="component-rod">
                        <li class=""><a href='@Url.Action("o_praksi","Admin")'><i class="icon-angle-right"></i> Podaci o praksi </a></li>
                        <li class=""><a href='@Url.Action("osobni_podaci","Admin")'><i class="icon-angle-right"></i> Osobni podaci </a></li>
                    </ul>
                </li>






               @Html.Partial("partial_ostalo")

            </ul>

        </div>
